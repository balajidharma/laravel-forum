{
    "name": "balajidharma/laravel-forum",
    "description": "Laravel forum - Create forums to your Laravel projects",
    "license": "MIT",
    "keywords": ["categories","laravel","laravel-forum"],
    "authors": [
        {
            "name": "Balaji Dharmaraja",
            "homepage": "https://github.com/balajidharma"
        }
    ],
    "require": {
        "illuminate/support": "^10.0|^11.0",
        "balajidharma/laravel-comment":"^1.0",
        "balajidharma/laravel-category":"^2.0",
        "oddvalue/laravel-drafts":"^2.0"
    },
    "autoload": {
        "psr-4": {
            "BalajiDharma\\LaravelForum\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BalajiDharma\\LaravelForum\\ForumServiceProvider"
            ]
        }
    }
}